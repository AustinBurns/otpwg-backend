{
  "name": "otpwg",
  "version": "1.0.0",
  "description": "This is the backend for 'Oh the Places We'll Go!' web application",
  "main": "src/index.js",
  "scripts": {
    "test": "NODE_ENV=testing jest",
    "start": "nodemon --exec babel-node -r node_modules/dotenv/config src/index.js",
    "build": "babel src --out-dir dist",
    "serve": "node dist/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AustinBurns/otpwg-backend.git"
  },
  "author": "Austin Burns",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AustinBurns/otpwg-backend/issues"
  },
  "homepage": "https://github.com/AustinBurns/otpwg-backend#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.3",
    "@types/jest": "^24.0.12",
    "acorn": "^6.1.1",
    "dotenv": "^7.0.0",
    "eslint": "^5.16.0",
    "eslint-plugin-jest": "^22.4.1",
    "jest": "^24.7.1",
    "nodemon": "^1.18.11",
    "prettier": "^1.17.0",
    "superagent": "^5.0.4",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "got": "^9.6.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.2.3",
    "mongoose": "^5.5.1",
    "morgan": "^1.9.1",
    "ramda": "^0.26.1"
  },
  "jest": {
    "verbose": true,
    "testURL": "http://localhost:8080/api/v1/",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "dist/"
    ]
  }
